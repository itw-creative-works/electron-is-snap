{
  "name": "electron-is-snap",
  "version": "1.0.4",
  "description": "Check if the Electron app is packaged as a Linux snap application",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "node -e 'require(`prepare-package`)()'",
    "prepare:watch": "nodemon -w ./src -e '*' --exec 'npm run prepare'"
  },
  "preparePackage": {
    "input": "./src",
    "output": "./dist",
    "replace": {}
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "author": "ITW Creative Works",
  "license": "MIT",
  "keywords": [
    "root",
    "electron",
    "path",
    "snap",
    "linux",
    "is snap"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itw-creative-works/electron-is-snap.git"
  },
  "bugs": {
    "url": "https://github.com/itw-creative-works/electron-is-snap/issues"
  },
  "homepage": "https://itwcreativeworks.com",
  "dependencies": {},
  "devDependencies": {
    "prepare-package": "^1.1.6"
  }
}